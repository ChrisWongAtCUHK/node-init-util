{
  "name": "node-init-util",
  "version": "1.1.0",
  "description": "Quick starter of NodeJS",
  "bin": {
    "node-init-util": "./index.js",
    "node-init": "./index.js"
  },
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "test": "mocha"
  },
  "_moduleAliases": {
    "@root": ".",
    "@lib": "lib"
  },
  "keywords": [
    "node",
    "init",
    "git",
    "husky",
    "eslint",
    "lint-staged"
  ],
  "main": "index.js",
  "author": "Chris Wong <wc38.wong@gmail.com",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "mocha": "^7.1.0",
    "tmp": "^0.1.0",
    "vuepress": "^1.4.0"
  },
  "dependencies": {
    "jsonfile": "^6.0.1",
    "module-alias": "^2.2.2",
    "puppeteer": "^2.1.1"
  },
  "repository": "git@github.com:ChrisWongAtCUHK/node-init-util.git"
}
